<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <!-- Attributes don't work in drawables. This is the same as: ?attr/colorBackgroundView -->
            <solid android:color="#FFFFFFFF"/>
        </shape>
    </item>

    <item
        android:left="12dp"
        android:right="12dp">

        <shape android:shape="rectangle">
            <solid android:color="#FFBEC8D2"/>
        </shape>
    </item>

</layer-list>
